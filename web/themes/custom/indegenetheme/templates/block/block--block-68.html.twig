						<img src="{{ logopath }}" alt="{{ site_name }}" title="{{ site_name }}">
					</a>
				</div>
				<div class="mobile-nav" style="height: 75px; line-height: 75px; width: 75px;">
					<i class="fa fa-bars"></i>
				</div>
				<!-- Main Menu -->
				<div class="inner-nav desktop-nav">
					<ul class="clearlist">
						{% set items = simplify_menu('main') %}
						{% set default_level = 1 %}
						{# {{kint(items)}} #}
