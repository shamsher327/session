{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/bootstrap": "^3.23",
        "drupal/bootstrap4": "^2.1",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/content_moderation_notifications": "^3.3",
        "drupal/core-composer-scaffold": "^8.2",
        "drupal/core-project-message": "^8.2",
        "drupal/core-recommended": "^8.2",
        "drupal/css_editor": "^1.3",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/diff": "^1.0",
        "drupal/dropdown_language": "2.7",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/examples": "^3.0",
        "drupal/features": "^3.12",
        "drupal/image_slider": "^1.3",
        "drupal/layout_builder_kit": "^1.0@beta",
        "drupal/layoutcomponents": "^3.0",
        "drupal/masquerade": "^2.0@beta",
        "drupal/moderation_dashboard": "^1.0@beta",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/rest_api_access_token": "^1.6",
        "drupal/restui": "^1.20",
        "drupal/search_api": "^1.20",
        "drupal/search_api_db": "^1.20",
        "drupal/slick": "^2.3",
        "drupal/token": "^1.9",
        "drupal/uikit_slideshow": "^2.1",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/views_slideshow": "^4.8",
        "drupal/viewsreference": "^1.7",
        "drupal/webform": "^6.0",
        "drush/drush": "^9.0",
        "rusticisoftware/tincan": "~1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
